                    <name>AC-Activation</name>
                    <type>DEPENDENT</type>
                    <key>lupus.ac_activation_ok</key>
                    <delay>0</delay>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
